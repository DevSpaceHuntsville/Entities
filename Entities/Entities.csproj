<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.2.1</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/DevSpaceHuntsville/Entities</RepositoryUrl>
    <PackageId>DevSpace.Common.Entities</PackageId>
    <AssemblyName>DevSpace.Common.Entities</AssemblyName>
    <RootNamespace>DevSpace.Common.Entities</RootNamespace>
    <Authors>Chris Gardner</Authors>
    <Company>DevSpace Huntsville</Company>
    <Description>Common entities to share across multiple projects</Description>
    <Copyright>2020</Copyright>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://dev.azure.com/devspacehuntsville/DevSpace</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Entities</PackageTags>
    <AssemblyVersion>0.2.1.0</AssemblyVersion>
    <FileVersion>0.2.1.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="9.0.1" />
  </ItemGroup>

</Project>
